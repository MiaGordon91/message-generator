{
    "version": 2,
    "builds": [
      {
        "src": "./server/index.tsx",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./server/index.tsx"
      }
    ],
    "rewrites": [{ "source": "./server/(.*)", "destination": "./server" }]
  }